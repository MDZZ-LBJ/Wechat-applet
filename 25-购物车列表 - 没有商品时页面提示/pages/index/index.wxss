.floor_group{

}
.floor_title{
  padding: 10rpx 0;
}
.floor_title image{
  width: 100%
}
.floor_list {
  /* 清除浮动 */
  overflow: hidden
}

.floor_list navigator{
  float: left;
  width: 33.3%
}
/* 后4张图片 */
.floor_list navigator:nth-last-child(-n+4){
   /* 33.33vw * 386 / 232 / 2 */
   height: 27.727vw ;
   border-left: 10rpx solid #fff
 }

floor_list navigator:nth-last-child(2),navigator:nth-last-child(3){
   border-bottom: 10rpx solid #fff
 }


.floor_list navigator image{
  width: 100%;
  height: 100%
}